{
  "name": "nebula",
  "version": "0.0.1",
  "description": "A WebGPU library for building 3D applications quickly",
  "author": "RE <re_dev@qq.com> (https://rerere.me)",
  "homepage": "https://github.com/re-ovo/nebula",
  "repository": "github:re-ovo/nebula",
  "license": "MIT",
  "keywords": [
    "webgpu",
    "3d",
    "game",
    "engine",
    "shader",
    "gpu"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc && vite build --watch",
    "start": "vite",
    "test": "vitest",
    "build": "tsc && vite build",
    "build:types": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier ./src --write"
  },
  "dependencies": {
    "wesl": "^0.6.0-rc3",
    "wgpu-matrix": "^3.4.0",
    "wgsl_reflect": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tailwindcss/vite": "^4.0.15",
    "@types/node": "^22.13.12",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.9",
    "@webgpu/types": "^0.1.58",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "globals": "^16.0.0",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.4.0",
    "tailwindcss": "^4.0.15",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9",
    "wesl-plugin": "^0.6.0-rc3"
  }
}
